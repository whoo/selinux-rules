
module auriga-systemd 3.0;

require {
	type initrc_t;
	type systemd_hostnamed_t;
	type systemd_tmpfiles_t;
	type var_log_t;
	class dbus send_msg;
	class dir { relabelfrom relabelto setattr };
}

#============= systemd_hostnamed_t ==============
allow systemd_hostnamed_t initrc_t:dbus send_msg;

#============= systemd_tmpfiles_t ==============
allow systemd_tmpfiles_t var_log_t:dir relabelfrom;
allow systemd_tmpfiles_t var_log_t:dir relabelto;
allow systemd_tmpfiles_t var_log_t:dir setattr;
